{
  "version": 2,
  "builds": [
  
    {
      "src": "frontend_tripzo/index.html", 
      "use": "@vercel/static"            
    },

    {
      "src": "backend_tripzo/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend_tripzo/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "frontend_tripzo/$1"
    }
  ]
}